package practiceMKT;

public class ConstructorXDatatypes {

	ConstructorXDatatypes() {

		System.out.println("no para");

	}

	ConstructorXDatatypes(int a) {

		System.out.println(a);

	}

	 ConstructorXDatatypes(String b) {

		System.out.println(b);

	}

	 ConstructorXDatatypes(Boolean c) {

		System.out.println(c);

	}

	 ConstructorXDatatypes(double d) {

		System.out.println(d);

	}

	 ConstructorXDatatypes(float f) {

		System.out.println(f);

	}

	public static void main(String[] args) {

		int a = 0;
		String b = null;
		Boolean c = true;
		double d = 2.2;
		float f = 1.1f;
		new ConstructorXDatatypes();
		new ConstructorXDatatypes(a);
		new ConstructorXDatatypes(b);
		new ConstructorXDatatypes(c);
		new ConstructorXDatatypes(d);
		new ConstructorXDatatypes(f);

	}

}
