package assignmentMKT;

public class PracticeConstructor {

	public static void main(String[] args) {

		PracticeConstructor cons1 = new PracticeConstructor(5, "          ");
		PracticeConstructor cons2 = new PracticeConstructor("          ", 5);
		new PracticeConstructor(5,"NIDHI");

		cons1.mymethod();
		cons2.mymethod2();

	}

	PracticeConstructor(int a, String b)

	{

		System.out.println("main caled me as constructure" + b + a);

	}

	PracticeConstructor(String b, int a)

	{

		System.out.println("main caled me as constructure by overloading" + b + a);

	}

	static void mymethod()

	{

		System.out.println("main caled me as a static method");
	}
   
	
	 void mymethod2()

	{

		System.out.println("main caled me as I am a non static method");
	}
}
